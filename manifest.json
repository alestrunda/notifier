{
  "name": "Notifier",
  "version": "1.0",
  "description": "Simple Chrome extension to periodically play notification sound.",
  "permissions": ["storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "options_page": "index.html",
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "web_accessible_resources": [
    "audio/goes-without-saying-608.mp3",
    "audio/open-your-eyes-and-see-323.mp3"
  ],
  "manifest_version": 2
}