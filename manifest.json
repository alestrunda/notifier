{
  "name": "Notifier",
  "version": "1.0",
  "description": "Simple Chrome extension to periodically play notification sound.",
  "permissions": ["storage"],
  "background": {
    "page": "background.html"
  },
  "options_page": "index.html",
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "web_accessible_resources": [
    "audio/goes-without-saying-608.mp3",
    "audio/juntos-607.mp3",
    "audio/open-your-eyes-and-see-323.mp3",
    "audio/quite-impressed-565.mp3",
    "audio/slow-spring-board-longer-tail-571.mp3"
  ],
  "manifest_version": 2
}